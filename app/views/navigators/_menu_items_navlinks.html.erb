      navigator.add_link do |link|
        link.text = (t("ubiquo.design.sitemap"))
        link.url = ubiquo_menu_items_path    
        link.highlights_on({:controller => "ubiquo/menu_items"})
      end 
    end
%>
<%= render_link_navigator(navigator_section) %>
